git config --global --get alias.st &>/dev/null || git config --global alias.st status
git config --global --get alias.dc &>/dev/null || git config --global alias.dc "diff --cached"
git config --global --get alias.lg &>/dev/null || git config --global alias.lg "log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit"
git config --global --get alias.ca &>/dev/null || git config --global alias.ca "commit --ammend"
